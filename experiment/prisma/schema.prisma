  @@index([affiliate_id], map: "affiliate_id")
  @@index([dealType], map: "dealType")
}

model affiliates_msgs {
  id                     Int      @unique(map: "id") @default(autoincrement())
  rdate                  DateTime @db.DateTime(0)
  valid                  Int
  admin_id               Int
  group_id               Int
  affiliate_id           Int      @default(-1)
  advertiser_id          Int      @default(-1)
