  merchantid    Int     @default(0)

  @@index([campID, affiliateID], map: "campID")
}

model affiliates_deals {
  id           Int                        @unique(map: "id") @default(autoincrement())
  rdate        DateTime                   @db.DateTime(0)
  admin_id     Int
  merchant_id  Int
  affiliate_id Int
