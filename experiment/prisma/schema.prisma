model short_urls {
  id         Int      @id @default(autoincrement()) @db.UnsignedInt
  long_url   String   @db.VarChar(610)
  short_code Bytes    @db.VarBinary(6)
  short_urls DateTime @default(now()) @db.Timestamp(0)
  counter    Int      @default(0) @db.UnsignedInt

  @@index([short_code], map: "short_code")
}

model stats_banners {
  id           Int      @unique(map: "id") @default(autoincrement())
  rdate        DateTime @db.Date
  ctag         String   @db.Char(15)
  affiliate_id Int
  group_id     Int
  merchant_id  Int
  banner_id    Int
  profile_id   Int
  views        Int
  clicks       Int

  @@index([affiliate_id], map: "affiliate_id")
  @@index([banner_id], map: "banner_id")
  @@index([banner_id], map: "creative_id")
  @@index([ctag], map: "ctag")
  @@index([merchant_id], map: "merchant_id")
  @@index([profile_id], map: "profile_id")
}

model sub_banners {
