model traders_tag {
  id            Int                @unique(map: "id") @default(autoincrement())
  valid         Int
  added_by      Int
  rdate         DateTime           @db.DateTime(0)
  merchant_id   Int
  trader_id     Int
  revenue       Float
  admin_revenue Float
  status        traders_tag_status
  notes         String             @db.Text
  calReport     Int

  @@unique([merchant_id, trader_id], map: "merchant_id")
  @@index([merchant_id], map: "merchant_id_2")
  @@index([trader_id], map: "trader_id")
}

model traffic {
  id             Int                @id @default(autoincrement())
  rdate          DateTime           @default(now()) @db.DateTime(0)
  unixRdate      Int
  ctag           String             @db.VarChar(85)
  uid            String             @db.VarChar(35)
  ip             String             @db.VarChar(35)
  admin_id       Int
  affiliate_id   Int
  group_id       Int
  banner_id      Int
  merchant_id    Int
  profile_id     Int
  language_id    Int
  promotion_id   Int
  last_update    DateTime           @db.DateTime(0)
  valid          Boolean            @default(true)
  title          String             @db.VarChar(254)
  bannerType     traffic_bannerType
  type           traffic_type       @default(traffic)
  width          Int
  height         Int
  file           String             @db.VarChar(100)
  url            String             @db.VarChar(200)
  alt            String             @db.VarChar(100)
  platform       traffic_platform
  os             String             @db.VarChar(100)
  osVersion      String             @db.VarChar(50)
  browser        String             @db.VarChar(40)
  broswerVersion String             @db.VarChar(20)
  userAgent      String             @db.VarChar(100)
  country_id     String             @db.VarChar(2)
  refer_url      String             @db.VarChar(254)
  param          String             @db.VarChar(254)
  param2         String             @db.VarChar(254)
  param3         String             @db.VarChar(170)
  param4         String             @db.VarChar(170)
  param5         String             @db.VarChar(170)
  views          Int
  clicks         Int
  product_id     Int

  @@index([affiliate_id], map: "affiliate_id")
  @@index([merchant_id, banner_id], map: "banner_merchant")
  @@index([merchant_id], map: "merchant_id")
  @@index([product_id], map: "product_id")
  @@index([rdate], map: "rdate")
  @@index([uid], map: "uid")
  @@index([unixRdate], map: "unixRdate")
  @@index([unixRdate], map: "unixdate")
}

