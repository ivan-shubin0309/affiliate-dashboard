}

model affiliates_notes {
  id           Int                     @unique(map: "id") @default(autoincrement())
  rdate        DateTime                @db.DateTime(0)
  valid        Int
  admin_id     Int
  edited_by    Int
  affiliate_id Int
  group_id     Int
