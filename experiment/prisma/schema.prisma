  @@index([affiliate_id, merchant_id, rdate], map: "af_mer_date")
}

model affiliates_status {
  id                  Int      @unique(map: "id") @default(autoincrement())
  rdate               DateTime @db.DateTime(0)
  valid               Int
  title               String   @db.VarChar(254)
  created_by_admin_id Int

  @@index([title], map: "title")
  @@index([valid], map: "valid")
}

model affiliates_tickets {
  id           Int                       @id @default(autoincrement())
  rdate        DateTime                  @db.DateTime(0)
  last_update  DateTime                  @db.DateTime(0)
  ticket_id    Int
  affiliate_id Int
  admin_id     Int
  reply_email  String                    @db.VarChar(254)
  group_id     Int
