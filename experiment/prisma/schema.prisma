  ip                              String                   @db.VarChar(15)
  group_id                        Int
  refer_id                        Int
  refer_banner_id                 Int
  username                        String                   @db.VarChar(254)
  password                        String                   @db.VarChar(254)
  first_name                      String                   @db.VarChar(254)
  last_name                       String                   @db.VarChar(254)
  mail                            String                   @db.VarChar(254)
  website                         String                   @db.VarChar(254)
  website2                        String                   @db.VarChar(254)
  website3                        String                   @db.VarChar(254)
  phone                           String                   @db.VarChar(15)
