  subAffiliateID    Int?
  status            Int?
  updated           DateTime? @db.DateTime(0)

  @@id([merchantID, affiliateID, traderID, transactionID, Type, Amount, Date])
  @@index([Date], map: "Date")
  @@index([DealType], map: "DealType")
  @@index([subAffiliateID], map: "subAffiliateID")
}

model config_api_n_feeds {
  id              Int      @id @default(autoincrement())
