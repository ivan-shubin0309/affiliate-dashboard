  @@index([admin_id], map: "admin_id")
  @@index([affiliate_id], map: "affiliate_id")
  @@index([merchantID], map: "merchantID")
  @@index([ticket_id], map: "ticket_id")
}

model affiliates_traffic {
  id           Int      @unique(map: "id") @default(autoincrement())
  rdate        DateTime @db.DateTime(0)
  ip           String   @db.VarChar(15)
  affiliate_id Int
  profile_id   Int      @default(0)
  merchant_id  Int
  refer_url    String   @db.VarChar(255)
  visits       Int
  uid          String   @default("0") @db.VarChar(35)

  @@index([affiliate_id], map: "affiliate_id")
  @@index([refer_url, affiliate_id, profile_id], map: "forClicksSum")
  @@index([merchant_id], map: "merchant_id")
  @@index([refer_url], map: "refer_url_2")
}

model apicredentials {
  id          Int                 @id @default(autoincrement())
  merchant_id Int
  valid       Boolean
  user        String              @db.VarChar(50)
  password    String              @db.VarChar(50)
  url         String              @db.VarChar(100)
  type        apicredentials_type

  @@index([merchant_id], map: "merchant_id")
}

model chart_data {
  id         Int              @id @unique(map: "id") @default(autoincrement())
