}

model affiliates_profiles {
  id             Int      @unique(map: "id") @default(autoincrement())
  rdate          DateTime @db.DateTime(0)
  valid          Int
  affiliate_id   Int
  name           String   @db.VarChar(254)
  url            String   @db.VarChar(254)
