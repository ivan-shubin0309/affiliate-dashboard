generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model admins {
  id                 Int             @unique(map: "id") @default(autoincrement())
  notifyOnAffReg     Int             @default(0)
  rdate              DateTime        @db.DateTime(0)
  ip                 String          @db.VarChar(15)
  chk_ip             String          @db.VarChar(15)
  valid              Int
  lang               String          @db.VarChar(3)
  level              admins_level
  username           String          @unique(map: "username") @db.VarChar(254)
  password           String          @db.Char(32)
  first_name         String          @db.VarChar(254)
  last_name          String          @db.VarChar(254)
  email              String          @db.VarChar(254)
  lastactive         DateTime        @db.DateTime(0)
  logged             Int
  group_id           Int
  phone              String          @db.VarChar(254)
  IMUserType         String          @db.VarChar(50)
  IMUser             String          @db.VarChar(254)
  zopimChat          String          @db.Text
  bigPic             String          @db.VarChar(254)
  relatedMerchantID  String          @db.VarChar(20)
  preferedCurrency   String          @default("USD") @db.VarChar(3)
  userType           admins_userType @default(default)
  showAdditionalLink Boolean         @default(false)
  additionalLinkText String          @db.VarChar(80)
  additionalLinkUrl  String          @db.VarChar(90)
}

model admins_notes {
  id        Int      @unique(map: "id") @default(autoincrement())
  rdate     DateTime @db.DateTime(0)
  valid     Int
  admin_id  Int
  edited_by Int
  admins_id Int
  notes     String   @db.Text

  @@index([admins_id], map: "affiliate_id")
  @@index([valid], map: "valid")
}

model affiliate_messages_approval {
  id            Int      @id @default(autoincrement())
  affiliate_id  Int
  message_id    Int
  approval_date DateTime @default(now()) @db.DateTime(0)
}

model affiliates {
