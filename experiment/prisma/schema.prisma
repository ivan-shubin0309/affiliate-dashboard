model affiliates_campaigns_relations {
  id            Int     @id @default(autoincrement())
  name          String  @default("") @db.VarChar(40)
  campID        String  @db.VarChar(20)
  affiliateID   Int     @default(0)
  advertiserID  Int     @default(0)
  profile_id    Int     @default(0)
